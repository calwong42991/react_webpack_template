{
  "name": "react_webpack_template",
  "version": "1.0.0",
  "description": "react with webpack broiler plate",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --mode=development --config React-Client/config/webpack.base.config.js --open --hot --history-api-fallback --env.PLATFORM=local --env.VERSION=stag",
    "prebuild": "webpack --mode=production --config React-Client/config/webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "build": "node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calwong42991/react_webpack_template.git"
  },
